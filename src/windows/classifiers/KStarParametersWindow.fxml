<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="windows.classifiers.KStarParametersWindow">
	<children>
		<!-- ########################### -->
		<!--            title            -->
		<!-- ########################### -->
		
		<Label layoutX="20.0" layoutY="8.0" text="Customize Parameters">
			<font>
				<Font name="System Bold" size="12.0" />
			</font>
		</Label>
		
		<Separator layoutX="20.0" layoutY="32.0" prefHeight="3.0" prefWidth="440.0" />
		
		<!-- ########################### -->
		<!--          Parameters         -->
		<!-- ########################### -->
		
		<Label layoutX="146.0" layoutY="47.0" text="Batch size" />
		<Label layoutX="163.0" layoutY="81.0" text="Debug" />
		<Label layoutX="66.0" layoutY="116.0" text="Do not check capabilities" />
		<Label layoutX="93.0" layoutY="149.0" text="Entropic auto-blend" />
		<Label layoutX="131.0" layoutY="182.0" text="Global blend" />
		<Label layoutX="91.0" layoutY="248.0" text="Num Decimal Places" />
		<Label layoutX="123.0" layoutY="215.0" text="Missing Mode" />
		
		<TextField fx:id="batchSizeField" layoutX="210.0" layoutY="43.0" prefHeight="25.0" prefWidth="241.0" promptText="batchSize" />
		<ToggleButton fx:id="debugButton" layoutX="210.0" layoutY="77.0" mnemonicParsing="false" onAction="#handleToggleButtons" prefHeight="25.0" prefWidth="108.0" text="False" />
		<ToggleButton fx:id="doNotCheckCapabilitiesButton" layoutX="210.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleToggleButtons" prefHeight="25.0" prefWidth="108.0" text="False" />
		<ToggleButton fx:id="entropicAutoBlendButton" layoutX="210.0" layoutY="145.0" mnemonicParsing="false" onAction="#handleToggleButtons" prefHeight="25.0" prefWidth="108.0" text="False" />
		<TextField fx:id="globalBlendField" layoutX="210.0" layoutY="178.0" prefHeight="25.0" prefWidth="241.0" promptText="minNumObj" />
		<ComboBox fx:id="missingModeCombo" layoutX="210.0" layoutY="211.0" prefHeight="25.0" prefWidth="241.0" />
		<TextField fx:id="numDecimalPlacesField" layoutX="210.0" layoutY="244.0" prefHeight="25.0" prefWidth="241.0" promptText="numDecimalPlaces" />
		
		
		
		<!-- ########################### -->
		<!--           Buttons           -->
		<!-- ########################### -->
		
		<Separator layoutX="20.0" layoutY="656.0" prefHeight="3.0" prefWidth="440.0" />
		
		<Button fx:id="def" layoutX="116.0" layoutY="666.0" mnemonicParsing="false" onAction="#handleDefault" prefHeight="25.0" prefWidth="60.0" text="Default" />
		<Button fx:id="ok" layoutX="219.0" layoutY="666.0" mnemonicParsing="false" onAction="#handleOk" prefHeight="25.0" prefWidth="60.0" text="OK" />
		<Button fx:id="cancel" layoutX="324.0" layoutY="666.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="25.0" prefWidth="60.0" text="Cancel" />

	</children>
</AnchorPane>
