			<font>
				<Font name="System Bold" size="12.0" />
			</font>
		</Label>
		
		<Label fx:id="pageReference" layoutX="602.0" layoutY="34.0" text="1/3" />

		<Separator layoutX="20.0" layoutY="60.0" prefHeight="3.0" prefWidth="600.0" />
		
		<VBox fx:id="helpLabel" layoutX="20.0" layoutY="75.0" prefHeight="330.0" prefWidth="600.0" />
		
		<Separator layoutX="20.0" layoutY="420.0" prefHeight="3.0" prefWidth="600.0" />
		
